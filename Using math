#include <iostream>
#include <cmath>
using namespace std;

int mergeNumbers(int a, int b) {
    int temp = b;
    int digits = 0;

    // Count digits in b
    while (temp > 0) {
        temp /= 10;
        digits++;
    }

    // Merge: shift a left and add b
    int merged = a * pow(10, digits) + b;
    return merged;
}

int main() {
    int a = 123, b = 456;
    cout << "Merged number: " << mergeNumbers(a, b) << endl;
    return 0;
}
